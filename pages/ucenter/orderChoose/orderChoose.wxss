/* pages/ucenter/orderChoose/orderChoose.wxss */
/*ba*/
.choose_ba{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  z-index: -1;
}
/*process*/
.team_box{
  margin: 20rpx;
  border-radius: 20rpx;
  padding: 50rpx 20rpx;
}
.team_process{
  text-align: center;
  margin-bottom: 180rpx;
}
.process_num{
  display: inline-block;
  color: #999;
  width: 120rpx;
  height: 120rpx;
  text-align: center;
  border-radius: 80rpx;
  line-height: 60rpx;
  margin: 0rpx 50rpx;
  padding: 10rpx;
}
.process_num_on{
  color: #fff;
  background-color: rgba(255, 225, 255, 0.5);
}
.process_num_on>.process_img{
  width: 100rpx;
  height: 100rpx;
}
.process_img{
  display: block;
  width: 80rpx;
  height: 80rpx;
  margin: auto;
}
.process_text{
  color: #fff;
  font-size: 24rpx;
  margin-top: 10rpx;
}
.type_val{
  display: inline-block;
  font-size: 24rpx;
  color: #008cd7;
  background-color: #d8f1ff;
  padding: 5rpx 15rpx;
  border-radius:10rpx ;
  margin: 10rpx 10rpx;
}
/*choose*/
.choose_box{
  margin: 20rpx;
  border-radius: 20rpx;
  padding:50rpx 20rpx;
}
.choose_process{
  text-align: center;
  margin-bottom: 80rpx;
}
.checkbox_li{
  overflow: hidden;
  margin: 20rpx;
  padding-top: 20rpx;
}
.checkbox_val{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  border: 15rpx solid #cdcdcd;
  border-radius: 10rpx;
  float: left;
}
.checkbox_val_on{
  border: 15rpx solid #008cd7;
}
.checkbox_text{
  float: left;
  padding-left: 30rpx;
}
.choose_option{
  margin: 40rpx 0rpx;
}
.floor_val{
  font-size: 24rpx;
  padding: 20rpx 100rpx;
  border: 1px solid #008cd7;
  color: #008cd7;
  border-radius:10rpx ;
  width: 200rpx;
}
/*two*/
.live_info{
  padding:0rpx 30rpx;
}
.live_input{
  padding: 20rpx 0rpx;
}
.live_title{
  text-align: center;
  padding: 30rpx;
  border-bottom: 1px solid #999;
}
.input_li{
  padding: 20rpx 0rpx;
  position: relative;
}
.input_li text{
  display: inline-block;
  width: 120rpx;
  line-height: 45rpx;
  text-align: right;
  float: left;
  padding-top:10rpx ;
}
.input_li input{
  margin-left: 120rpx;
  background-color: #f1f1f1;
  padding: 10rpx 20rpx;
  border-radius:10rpx ;
}
.room_ul{
  overflow: hidden;
}
.room_li{
  width: 25%;
  float: left;
  padding: 10rpx 0rpx;
  
}
.room_detail{
  font-size: 28rpx;
  color: #999;
  padding: 10rpx;
}
.room_li_val{
  color: #008cd7;
  background-color: #d8f1ff;
  font-size: 28rpx;
  text-align: center;
  padding: 10rpx 0rpx;
  border-radius:20rpx ;
  margin: 0rpx 10rpx;
}
.room_li_on{
  color: #fff;
  background-color: #008cd7;
}
/*btn*/

.btn_define{
  background: linear-gradient(to right, #039cee , #753ffd);
  color: #fff;
  border-radius: 40rpx;
  text-align: center;
  padding: 20rpx;
  margin: 20rpx;
}
.btn_again{
  background-color: #e6e4e4;
  color: #666;
  border-radius: 40rpx;
  text-align: center;
  padding: 20rpx;
  margin: 20rpx;
}