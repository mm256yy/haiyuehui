page{
  background-color: #f1f1f1;
}
/*head*/
.ucenter_ba{
  width: 100%;
  /* position: absolute;
  top: 0rpx;
  left: 0rpx; 
  z-index: -1;*/
}
.ucenter_head{
  z-index: -1;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  /* margin-bottom: 50rpx; */
}
.ucenter_person{
  position: absolute;
  top: 140rpx;
  left: 200rpx;
  /* margin:140rpx 0rpx 0rpx 200rpx; */
  padding: 30rpx;
}
.ucenter_name{
  font-size: 34rpx;
  margin-right: 16rpx;
  color: #fff;
}
.ucenter_vip{
  color: #cdcdcd !important;
  font-size: 26rpx;
  color: #fff;
}
.ucenter_mobile_box{
  margin-top: 10rpx;
}
.ucenter_mobile{
  color: #999;
}
.ucenter_mobile_no{
  color: #fff;
}
.progress_box{
  display: inline-block;
  width: 300rpx;
  height: 10rpx;
  background-color: #fff;
  margin-top:30rpx ;
  border-radius:200rpx ;
  position: relative;
}
.progress_val{
  width: 20rpx;
  height: 10rpx;
  background: linear-gradient(to right, #039cee , #753ffd);
  border-radius:200rpx ;
}
.progress_text{
  position: absolute;
  top: -5rpx;
  right: -150rpx;
}
.ucenter_text{
  font-size: 22rpx;
  color: #cdcdcd !important;
  line-height: 40rpx;
}
.ucenter_tou{
  width: 150rpx;
  height: 150rpx;
  border-radius:150rpx ;
  position: absolute;
  top: 0rpx;
  left: -160rpx;
}
.ucenter_set{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}
/*menu*/
.ucenter_menu{
  margin-top: 400rpx;
  width: 100%;
  z-index: 2;
}
.menu_box{
  margin: 20rpx;
  padding:20rpx 20rpx;
  border-radius: 20rpx;
  background-color: #fff;
  box-shadow: 0rpx 5rpx 8rpx #f1f1f1;
}
.menu_ul{
  overflow: hidden;
}
.menu_li{
  width: 25%;
  float: left;
  text-align: center;
  margin:20rpx 0rpx;
}
.menu_li text{
  color: #313847;
  font-size: 24rpx;
}
.menu_icon{
  width: 100rpx;
  height: 100rpx;
  border-radius:100rpx ;
  margin:auto;
  margin-bottom:10rpx ;
  position: relative;
}
.menu_icon image{
  width: 100rpx;
  height: 100rpx;
  margin: 0rpx;
}
.badge_num{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  border-radius:430rpx ;
  border: 1px solid #f00000;
  background-color: #f00000;
  color: #fff !important;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  line-height: 35rpx;
}
/*news*/
.news_box{
  background: linear-gradient(to right, #039cee , #346ae8);
  /* background-color: #039cee; 753ffd  */
  margin: 0rpx 20rpx;
  border-radius: 20rpx;
  box-shadow: 0rpx 5rpx 8rpx #f1f1f1;
  margin-bottom: 20rpx;
  padding: 20rpx;
  position: relative;
}
.news_title{
  color: #fff;
  font-size: 32rpx;
}
.news_label{
  color: #b6e5ff;
}
.news_btn{
  width: 180rpx;
  text-align: center;
  padding: 13rpx;
  margin: 25rpx 0rpx 25rpx 20rpx;
  color: #fff;
  border:1rpx solid #fff ;
  border-radius: 50rpx;
  font-size: 24rpx;
  position: absolute;
  top: 5rpx;
  right: 20rpx;
}
/*member*/
.ucenter_member{
  margin: 20rpx;
  padding: 20rpx;
  background-color: #313847;
  box-shadow: 0rpx 5rpx 8rpx #f1f1f1;
  border-radius:20rpx;
  position: relative;
}
.member_kind{
  width: 450rpx;
  height: 450rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  z-index: 1;
  opacity: 0.1;
}
.member_h1{
  text-align: center;
  color: #fff;
  font-size: 34rpx;
  margin-bottom: 30rpx;
}
.member_ul{
  overflow: hidden;
}
.member_li{
  width: 25%;
  text-align: center;
  float: left;
  margin:20rpx 0rpx;
}
.member_li_img{
  width: 80rpx;
  height: 80rpx;
}
.member_li_text{
  color: #fff;
  font-size: 22rpx;
}
/*商城*/
.ucenter_market{
  background-color: #fff;
  margin: 20rpx;
  padding: 20rpx;
  border-radius:20rpx ;
  box-shadow: 0rpx 5rpx 8rpx #f1f1f1;
  overflow: hidden;
}
.market_head{
  padding: 10rpx 0rpx;
}
.head_h1{
  font-size: 30rpx;
}
.head_h2{
  float: right;
}
.market_ul{
  margin-top: 20rpx;
  padding-top: 20rpx;
  border-top: 1rpx solid #f1f1f1;
}
.maket_li{
  width: 33%;
  float: left;
}
.maket_li_product{
  padding: 10rpx;
  background-color: #fff;
}
.goods_img_box{
  width: 100%;
}
.goods_img{
  width: 100%;
  height: 200rpx;
  background-color: #f1f1f1;
}
.goods_text{
  overflow: hidden;
}
.goods_t1{
  height: 3em;
  color: #666;
  overflow: hidden;
}
.goods_t2{
  color: #e66465;
}
.goods_t3{
  text-decoration: line-through;
  color: #cdcdcd;
  padding-left: 20rpx;
}
/*other*/
.ucenter_other{
  background-color: #fff;
  margin: 0rpx 20rpx;
  border-radius:20rpx ;
  box-shadow: 0rpx 5rpx 8rpx #f1f1f1;
}
.other_ul{
  padding: 0rpx 20rpx;
  margin-bottom: 60rpx;
}
.other_li{
  padding: 30rpx 0rpx;
  border-bottom:1rpx solid #f1f1f1 ;
  position: relative;
}
.other_li_img{
  width: 40rpx;
  height: 40rpx;
  float: left;
}
.other_li_text{
  padding-left: 20rpx;
}
.other_li_badge{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  border-radius:40rpx ;
  background-color: #ff4747;
  text-align: center;
  position: absolute;
  top: 28rpx;
  right: 60rpx;
}
.other_li_badge_num{
  color: #fff;
  font-size: 24rpx;
  line-height: 35rpx;
}
.other_li_go{
  width: 40rpx;
  height: 40rpx;
  float: right;
}
/*我的服务*/
.ucenter_service_h1{
  text-align: center;
  font-size: 34rpx;
  padding: 10rpx;
}
.service_ul{
  background-color: #fff;
  border-radius:20rpx;
  margin: 20rpx;
  padding: 20rpx;
  overflow: hidden;
}
.service_li{
  width: 25%;
  text-align: center;
  float: left;
  margin-bottom: 20rpx;
}
.service_img{
  width: 80rpx;
  height: 80rpx;
}
.service_text{
  text-align: center;
  font-size: 24rpx;
}
