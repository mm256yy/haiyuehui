.result_head{
  text-align: center;
  padding:50rpx ;
}
.logo{
  width: 140rpx;
  height: 140rpx;
  margin:20rpx auto ;
}
.result_success{
  color: #039cee;
  font-size: 34rpx;
}
.result_fail{
  font-size: 34rpx;
  color: #cc0000;
}
/*body*/
.result_body{
  padding:20rpx ;
}
.result_li{
  padding:10rpx 0rpx;
}
.result_li_t1{
  color: #999;
}
.result_li_t2{
  float: right;
}
/*foot*/
.result_foot{
  padding:20rpx ;
}
.result_btn{
  background:linear-gradient(to right, #039cee , #753ffd);
  color: #fff;
  text-align: center;
  padding:20rpx;
  border-radius:50rpx ;
}
.result_btn_fail{
  background-color: #cdcdcd;
  color: #fff;
  text-align: center;
  padding:20rpx;
  border-radius:50rpx ;
}