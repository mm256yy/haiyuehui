page{
  background-color: #f1f1f1;
}
/*from*/
.form_ul {
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 0 30rpx;
  background: #fff;
}
.form_li{
  height: 96rpx;
  position: relative;
  border-bottom: 1px solid #f1f1f1;
  display: flex;
}
.form_li_t{
  width: 200rpx;
  line-height: 95rpx;
}
.form_li:last-child{
  border-bottom: none;
}
.form_input{
  flex: 1 1 auto;
  display: block;
  width: 100%;
  height: 95rpx;
  color: #333;
  font-size: 30rpx;
}
/*radio*/
.radio_box{
  padding: 30rpx 0rpx;
}
.form_mo{
  float: left;
  color: #999;
}
.agree_radio{
  float: left;
}
.form_clean{
  color: #999;
  position: absolute;
  top: 30rpx;
  right: 20rpx;
}
.form_del{
  color: #cc0000;
  position: absolute;
  top: 30rpx;
  right: 20rpx;
}
/*btn*/
.btn_box{
  padding:0rpx 30rpx;
  position: fixed;
  bottom: 10rpx;
  left: 0rpx;
  width: 100%;
  background-color: #f1f1f1;
}
.btn_success{
  margin: 20rpx 0rpx 20rpx 0rpx;
  padding: 20rpx 10rpx;
  border-radius:50rpx ;
  color: #fff;
  font-size: 30rpx;
  width: 100%;
  border-radius: 50rpx;
  background: linear-gradient(to right, #039cee , #753ffd);
  text-align: center;
}