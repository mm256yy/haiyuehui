/*from*/
.form_ul {
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 0 30rpx;
  background: #fff;
}
.form_li{
  height: 96rpx;
  position: relative;
  border-bottom: 1px solid #d9d9d9;
  display: flex;
}
.form_li_t{
  width: 200rpx;
  line-height: 95rpx;
}
.form_input{
  flex: 1 1 auto;
  display: block;
  width: 100%;
  height: 95rpx;
  color: #333;
  font-size: 30rpx;
  border-bottom: 1px solid #f1f1f1;
}
.form_mobile{
  color: #cdcdcd;
}
/*btn*/
.btn_box{
  padding:0rpx 30rpx;
}
.btn_success{
  margin: 60rpx 0rpx 40rpx 0rpx;
  padding: 20rpx 10rpx;
  border-radius:50rpx ;
  color: #fff;
  font-size: 30rpx;
  width: 100%;
  border-radius: 50rpx;
  background: linear-gradient(to right, #039cee , #753ffd);
  text-align: center;
}
/*template*/
@import "/template/template.wxss";