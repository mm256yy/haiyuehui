page{
  background-color: #f1f1f1;
}
/*head*/
.head{
  background-color: #039cee;
  padding: 40rpx;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
}
.head_points{
  margin-bottom: 40rpx;
}
.head_t1{
  color: #fff;
  font-size: 28rpx;
  font-weight: 800;
}
.head_t2{
  color: #fff;
  font-size: 50rpx;
  font-weight: 800;
}
.head_t3{
  color: #fff;
  font-size: 24rpx;
  margin: 0rpx 10rpx;
}
.head_t3:first-child{
  margin-left: 0rpx;
}
/*goods*/
.market_ul{
  margin-top: 265rpx;
  padding-bottom: 120rpx;
  overflow: hidden;
}
.maket_li{
  width: 50%;
  float: left;
}
.maket_li_product{
  border: 2rpx solid #f1f1f1;
  padding: 20rpx;
  background-color: #fff;
  border-radius: 10rpx;
}
.goods_img_box{
  width: 100%;
  text-align: center;
}
.goods_img{
  width: 100%;
  height: 340rpx;
  background-color: #f1f1f1;
  border-radius: 10rpx;
}
.goods_text{
  overflow: hidden;
}
.goods_t1{
  height: 3em;
  overflow: hidden;
  font-weight: 800;
}
.goods_t2{
  color: #e66465;
  font-weight: 800;
  font-size: 32rpx;
}
.goods_t3{
  color: #e66465;
  font-size: 24rpx;
}