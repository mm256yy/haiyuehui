.code_box{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  background-color: #f1f1f1;
  display: flex;
}
.code_qr{
  width: 90%;
  height: 700rpx;
  background-color: #fff;
  margin:auto ;
  text-align: center;
  padding: 50rpx;
  border-radius:20rpx ;
}
.code_qr_img{
  width: 100%;
}
.code_qr_text{
  color: #cdcdcd;
}