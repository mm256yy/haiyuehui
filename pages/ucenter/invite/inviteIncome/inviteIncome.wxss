/*head*/
.income_head{
  padding: 30rpx 30rpx 0rpx 30rpx;
  background-color: #fff;
}
.income_head_img{
  width: 100%;
  height: 200rpx;
  border-radius:20rpx ;
}
/*total*/
.income_total{
  padding: 20rpx;
}
.total_money{
  background-color: #fff9f7;
  border-radius:20rpx;
  text-align: center;
  padding: 30rpx 30rpx 50rpx 30rpx;
  margin: 10rpx;
}
.total_money_val{
  margin-right: 25rpx;
  margin-top: 10rpx;
}
.total_money_t1{
  color: #fe6f41;
  font-size: 24rpx;
}
.total_money_t2{
  color: #fe6f41;
  font-size: 40rpx;
}
.total_money_t3{
  color: #fe6f41;
  font-size: 28rpx;
  float: right;
}
.income_total{
  background-color: #fff;
}
.total_ul{
  display: flex;
  width: 100%;
}
.total_li{
  flex: 1 1 auto;
  margin: 10rpx;
  padding:20rpx 40rpx;
  border-radius:20rpx;
  background-color: #fff9f7;
  position: relative;
}
.total_li_money{
  color: #fe6f41;
  font-size: 40rpx;
}
.total_li_num{
  color: #fe6f41;
  font-size: 24rpx;
}
/*list*/
.income_list{
  border-top: 20rpx solid #f1f1f1;
}
.income_list_h{
  background-color: #fff;
  padding: 30rpx;
  border-bottom:1rpx solid #f1f1f1 ;
}
.income_list_h1{
  border-left:5rpx solid #fe6f41;
  padding-left:20rpx;
}
.income_data{
  background-color: #f1f1f1;
  padding:20rpx 30rpx;
  position: relative;
}
.income_data_choose{
  background-color: #fff;
  padding: 10rpx 20rpx 10rpx 20rpx;
  border-radius:50rpx;
  display: inline-block;
}
.total_li_cash_out{
  position: absolute;
  right: 30rpx;
  bottom: 30rpx;
  color: #fe6f41;
}
.income_ul{
  padding:0rpx 30rpx;
  background-color: #fff;
}
.income_li{
  padding: 20rpx;
  border-bottom: 1rpx solid #f1f1f1;
  position: relative;
}
.income_li_type{
  font-weight: 800;
}
.income_li_time{
  color: #cdcdcd;
}
.income_li_money{
  color:#fe6f41 ;
  font-size: 36rpx;
  position: absolute;
  top: 34rpx;
  right: 10rpx;
}
.income_most{
  text-align: right;
  float: right;
}