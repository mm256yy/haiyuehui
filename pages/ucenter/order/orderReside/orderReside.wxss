/*img*/
.cis_title{
  width: 100%;
}
/*two*/
.live_input{
  padding: 20rpx 30rpx;
  background-color: #fff;
}
.live_title{
  text-align: center;
  padding: 30rpx;
  border-bottom: 1px solid #999;
}
.form_ul{
  display: flex;
}
.form_left{
  flex: 1 1 auto;
}
.form_right{
  width: 180rpx;
  display: flex;
  border-left:1rpx solid #f1f1f1;
  border-bottom:1rpx solid #f1f1f1 ;
}
.form_person{
  align-items:center;
  text-align: center;
  margin: auto;
}
.person_img{
  width: 80rpx;
  height: 80rpx;
}
.person_text{
  color: #999;
  font-size: 24rpx;
}
.input_li{
  position: relative;
}
.input_li text{
  display: inline-block;
  width: 120rpx;
  line-height: 45rpx;
  text-align: right;
  float: left;
  padding-top:10rpx ;
}
.input_li input{
  padding: 25rpx 20rpx;
  border-radius:10rpx ;
  border-bottom:1rpx solid #f1f1f1 ;
}
.room_ul{
  overflow: hidden;
}
.room_li{
  width: 25%;
  float: left;
  padding: 10rpx 0rpx;
}
.room_detail{
  font-size: 24rpx;
  color: #999;
  padding: 10rpx;
}
.room_detai2{
  font-size: 24rpx;
  color: #753ffd;
  padding: 10rpx;
}
.room_li_val{
  color: #008cd7;
  background-color: #d8f1ff;
  font-size: 28rpx;
  text-align: center;
  padding: 10rpx 0rpx;
  margin: 0rpx 10rpx;
  overflow: hidden;
  border-radius:20rpx ;
  position: relative;
}
.room_li_on{
  color: #fff;
  background-color: #008cd7;
}
.room_choose{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 10rpx;
}
.room_choose_left{
  left: 0rpx;
}
.room_choose_right{
  right: 0rpx;
}
.live_red{
  color: #cc0000;
  font-size: 24rpx;
  background-color: #fff;
  padding: 0rpx 30rpx;
}
/*four*/
.choose_four_head{
  text-align: center;
  padding: 20rpx;
}
.room_introduce{
  display: inline-block;
  width: 25rpx;
  height: 25rpx;
  border-radius: 10rpx;
}
.room_introduce_name{
  color: #999;
  font-size: 24rpx;
  margin:5rpx ;
}
.choose_four_body{
  overflow-x: scroll;
  margin: 0rpx 20rpx;
}
.room_table{
  display: block;
  padding: 10rpx;
  margin:30rpx auto;
  border:5rpx solid #cdcdcd;
}
.room_li_tr{
  display: block;
}
.room_type{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  text-align: center;
  background-color: #fff;
  border-radius:5px;
  margin: 2px;
  position: relative;
}
.room_type_room{
  background-color: #c2e1ff;
}
.room_type_room_on{
  background-color: #1890ff;
}
.room_type_stairs{
  background-color: #a6c5a7;
}
.room_type_elevator{
  background-color: #ffce73;
}
.room_type_aisle{
  background-color: #fff;
}
.room_type_nothing{
  background-color: #cdcdcd;
}
.room_type_occupy{
  background-color: #dfacac;
}
.room_type_other{
  background-color: #f1f1f1;
}
.room_type_number{
  color: #fff;
  font-size: 20rpx;
  overflow: hidden;
}
.choose_four_room_val{
  text-align: center;
  color: #999;
  font-size: 28rpx;
}
.room_type_img{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 7rpx;
  left: 7rpx;
}
/*btn*/
.choose_btn{
  background-color: #fff;
}
.btn_define{
  background: linear-gradient(to right, #039cee , #753ffd);
  color: #fff;
  border-radius: 40rpx;
  text-align: center;
  padding: 20rpx;
  margin: 20rpx;
}
.btn_again{
  background-color: #e6e4e4;
  color: #666;
  border-radius: 40rpx;
  text-align: center;
  padding: 20rpx;
  margin: 20rpx;
}
.reside_btn{
  margin-top: 50rpx;
}