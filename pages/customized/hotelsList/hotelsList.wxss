page{
  background-color: #f1f1f1;
}
.hotels_ul{
  width: 100%;
  padding: 15rpx;
}
.hotels_li{
  background-color: #fff;
  border-radius:20rpx ;
  margin-bottom: 60rpx;
}
.hotels_li>image{
  border-top-left-radius:20rpx ;
  border-top-right-radius:20rpx ;
  width: 100%;
  height: 450rpx;
  background-color: #f1f1f1;
}
.hotels_text{
  padding: 20rpx;
  position: relative;
}
.hotels_text>view:nth-child(1){
  font-size: 32rpx;
  font-weight: 800;
}
.hotels_text_attr{
  margin-top: 20rpx;
  display: inline-block;
  font-size: 24rpx;
  background-color: #d8f1ff;
  color: #008cd7;
  padding: 5rpx 10rpx;
  border-radius:10rpx ;
  margin-right: 10rpx;
}
.hotels_text_cis{
  margin-top: 20rpx;
  display: inline-block;
  font-size: 24rpx;
  background-color: #ede6ff;
  color: #753ffd;
  padding: 5rpx 10rpx;
  border-radius:10rpx ;
  margin-right: 10rpx;
}
/*zero*/
.order_zero{
  text-align: center;
}
.zero_img{
  display: inline-block;
  width: 200rpx;
  height: 200rpx;
  margin: 200rpx auto;
}
.cis{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: 0rpx;
  right: 10rpx;
}