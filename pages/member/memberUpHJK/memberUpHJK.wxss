page{
  background-color: #f1f1f1;
}
.box{
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.ad_img{
  width: 100%;
}
.redeem_code{
  border-radius: 10rpx;
  margin: 30rpx;
  text-align: center;
}
.redeem_title{
  color: #fff;
  line-height: 60rpx;
  font-weight: 800;
}
.redeem_input{
  margin-top: 10rpx;
  border: 1rpx solid #fff;
  border-radius: 10rpx;
  line-height: 80rpx;
  height: 80rpx;
  color: #fff;
}
.form_input{
  line-height: 80rpx;
  height: 80rpx;
}
.redeem_btn{
  background-color: #ddca9a;
  color: #102b48;
  border-radius: 10rpx;
  margin-top: 50rpx;
  padding: 20rpx;
}
.font_box{
  margin:60rpx 30rpx;
  padding:60rpx 20rpx;
  line-height: 40rpx;
  border: 1rpx solid #fff;
  border-radius: 10rpx;
}
.font_w{
  color: #fff;
}