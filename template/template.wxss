/*zero*/
.zero{
  text-align: center;
}
.zero_img{
  display: inline-block;
  width: 200rpx;
  height: 200rpx;
  margin: 200rpx auto;
}
/*zero*/
.zeroSmall{
  text-align: center;
}
.zeroSmall_img{
  display: inline-block;
  width: 150rpx;
  height: 150rpx;
  margin: 20rpx auto;
}
/*label*/
.label_box{
  position: relative;
  padding: 20rpx 30rpx;
}
.i_label{
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 20rpx;
  left: 30rpx;
}
.label_text{
  font-size: 26rpx;
  color: #999;
  padding-left: 40rpx;
}
/*foot*/
.foot{
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  width: 100%;
  border-top: 2rpx solid #cdcdcd;
}
.foot_ul{
  width: 100%;
  display: flex;
  background-color: #fff;
}
.foot_li{
  flex: 1 1 auto;
  text-align: center;
  margin: auto;
}
.foot_li_img{
  width: 54rpx;
  height: 54rpx;
}
.foot_li_name{
  color: #666;
  font-size: 22rpx;
  margin-top: -5rpx;
}
